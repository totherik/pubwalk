{
    "pine": {},

    "mongodb": {
        "url": "mongodb://localhost:27017/default"
    },

    "middleware": {

        "db": {
            "enabled": true,
            "priority": 10,
            "module": {
                "name": "path:./middleware/db",
                "arguments": [{
                    "datasource": "config:mongodb"
                }]
            }
        },

        "logger": {
            "enabled": false
        },

        "fileNotFound": {
            "enabled": true,
            "priority": 130,
            "module": {
                "name": "path:./middleware/fourohfour"
            }
        },

        "serverError": {
            "enabled": true,
            "priority": 140,
            "module": {
                "name": "path:./middleware/fivehundo"
            }
        }

    }

}
