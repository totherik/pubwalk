{
    "pine": {},

    "mongodb": {
        "url": "mongodb://localhost:27017/default"
    },

    "foursquare": {
        "url": "http://localhost:54699"
    },

    "untappd": {
        "url": "http://localhost:54734"
    },

    "middleware": {

        "db": {
            "enabled": true,
            "priority": 10,
            "module": {
                "name": "path:./middleware/db",
                "arguments": [{
                    "datasource": "config:mongodb"
                }]
            }
        },

        "foursquare": {
            "enabled": true,
            "priority": 11,
            "module": {
                "name": "path:./middleware/fetch",
                "arguments": [{
                    "propertyName": "foursquare",
                    "hostname": "config:foursquare.url"
                }]
            }
        },

        "untappd": {
            "enabled": true,
            "priority": 11,
            "module": {
                "name": "path:./middleware/fetch",
                "arguments": [{
                    "propertyName": "untappd",
                    "hostname": "config:untappd.url"
                }]
            }
        },

        "logger": {
            "enabled": false
        },

        "fileNotFound": {
            "enabled": true,
            "priority": 130,
            "module": {
                "name": "path:./middleware/fourohfour"
            }
        },

        "serverError": {
            "enabled": true,
            "priority": 140,
            "module": {
                "name": "path:./middleware/fivehundo"
            }
        }

    }

}
